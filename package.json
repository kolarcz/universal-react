  "name": "universal-react",
  "version": "1.0.0",
  "description": "Universal React",
  "main": "run.js",
  "scripts": {
    "postinstall": "npm run build",
    "gencerts": "openssl req -x509 -newkey rsa:2048 -keyout ./certs/server.key -out ./certs/server.crt -days 36525 -subj \"/O=-\" -nodes",
    "lint": "node_modules/.bin/eslint --ext=.js,.jsx run.js src webpack || echo done",
    "flow": "node_modules/.bin/flow || echo done",
    "start:dev": "node run.js --server=development",
    "start": "node run.js --server=production",
    "build": "node run.js --build"
